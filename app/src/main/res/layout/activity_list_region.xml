<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context=".LugaresTuristicos.ListRegionActivity">


    <LinearLayout
      android:layout_width="414dp"
      android:layout_height="59dp"
      android:orientation="horizontal"
      tools:layout_editor_absoluteX="0dp"
      tools:layout_editor_absoluteY="0dp">

        <ImageButton
          android:id="@+id/imageButton4"
          android:layout_width="wrap_content"
          android:layout_height="79dp"
          app:srcCompat="@android:drawable/ic_menu_search"
          tools:layout_editor_absoluteX="4dp"
          tools:layout_editor_absoluteY="0dp" />

        <EditText
          android:id="@+id/editText"
          android:layout_width="312dp"
          android:layout_height="48dp"
          android:layout_marginTop="16dp"
          android:layout_marginBottom="8dp"
          android:ems="10"
          android:hint="Buscar Huarique"
          android:inputType="textPersonName"
          app:layout_constraintBottom_toTopOf="@+id/imageViewImagen"
          app:layout_constraintTop_toTopOf="parent"
          app:layout_constraintVertical_bias="0.0"
          tools:layout_editor_absoluteX="120dp" />
    </LinearLayout>

    <ImageView
      android:id="@+id/imageViewImagen"
      android:layout_width="362dp"
      android:layout_height="197dp"
      android:layout_alignParentStart="true"
      android:layout_alignParentEnd="true"
      android:layout_marginStart="8dp"
      android:layout_marginTop="52dp"
      android:layout_marginEnd="8dp"
      android:layout_marginBottom="8dp"
      android:adjustViewBounds="true"
      android:contentDescription="Descripcion del contenido de la imagen"
      android:padding="5dp"
      android:scaleType="fitXY"
      android:src="@drawable/costa"
      app:layout_constraintBottom_toTopOf="@+id/imageViewImagen2"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.562" />

    <TextView
      android:id="@+id/myImageViewText"
      android:layout_width="188dp"
      android:layout_height="79dp"
      android:layout_alignLeft="@+id/imageViewImagen"
      android:layout_alignTop="@+id/imageViewImagen"
      android:layout_alignRight="@+id/imageViewImagen"
      android:layout_alignBottom="@+id/imageViewImagen"
      android:gravity="center"
      android:text="COSTA"
      android:textColor="@android:color/background_light"
      android:textSize="24sp"
      app:layout_constraintBottom_toBottomOf="@+id/imageViewImagen"
      app:layout_constraintEnd_toEndOf="@+id/imageViewImagen"
      app:layout_constraintStart_toStartOf="@+id/imageViewImagen"
      app:layout_constraintTop_toTopOf="@+id/imageViewImagen" />


    <ImageView
      android:id="@+id/imageViewImagen2"
      android:layout_width="368dp"
      android:layout_height="197dp"
      android:layout_alignParentStart="true"
      android:layout_alignParentEnd="true"
      android:layout_marginStart="8dp"
      android:layout_marginEnd="8dp"
      android:layout_marginBottom="28dp"
      android:adjustViewBounds="true"
      android:contentDescription="Descripcion del contenido de la imagen"
      android:padding="5dp"
      android:scaleType="fitXY"
      android:src="@drawable/sierra"
      app:layout_constraintBottom_toTopOf="@+id/imageViewImagen3"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

    <TextView
      android:id="@+id/myImageViewText2"
      android:layout_width="188dp"
      android:layout_height="79dp"
      android:layout_alignLeft="@+id/imageViewImagen2"
      android:layout_alignTop="@+id/imageViewImagen2"
      android:layout_alignRight="@+id/imageViewImagen2"
      android:layout_alignBottom="@+id/imageViewImagen2"
      android:gravity="center"
      android:text="SIERRA"
      android:textColor="#000000"
      app:layout_constraintBottom_toBottomOf="@+id/imageViewImagen2"
      app:layout_constraintEnd_toEndOf="@+id/imageViewImagen2"
      app:layout_constraintStart_toStartOf="@+id/imageViewImagen2"
      app:layout_constraintTop_toTopOf="@+id/imageViewImagen2" />


    <ImageView
      android:id="@+id/imageViewImagen3"
      android:layout_width="368dp"
      android:layout_height="197dp"
      android:layout_alignParentStart="true"
      android:layout_alignParentEnd="true"
      android:layout_marginStart="8dp"
      android:layout_marginEnd="8dp"
      android:layout_marginBottom="20dp"
      android:adjustViewBounds="true"
      android:contentDescription="Descripcion del contenido de la imagen"
      android:padding="5dp"
      android:scaleType="fitXY"
      android:src="@drawable/selva"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

    <TextView
      android:id="@+id/myImageViewText3"
      android:layout_width="188dp"
      android:layout_height="79dp"
      android:layout_alignLeft="@+id/imageViewImagen3"
      android:layout_alignTop="@+id/imageViewImagen3"
      android:layout_alignRight="@+id/imageViewImagen3"
      android:layout_alignBottom="@+id/imageViewImagen3"
      android:gravity="center"
      android:text="SELVA"
      android:textColor="#000000"
      app:layout_constraintBottom_toBottomOf="@+id/imageViewImagen3"
      app:layout_constraintEnd_toEndOf="@+id/imageViewImagen3"
      app:layout_constraintStart_toStartOf="@+id/imageViewImagen3"
      app:layout_constraintTop_toTopOf="@+id/imageViewImagen3" />


</android.support.constraint.ConstraintLayout>