<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context=".Principal.LoginActivity">

  <ImageView
    android:id="@+id/imageView"
    android:layout_width="323dp"
    android:layout_height="154dp"
    android:layout_marginStart="8dp"
    android:layout_marginTop="28dp"
    android:layout_marginEnd="8dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:srcCompat="@drawable/turismogo" />

  <ImageView
    android:id="@+id/imageView4"
    android:layout_width="398dp"
    android:layout_height="253dp"
    android:layout_marginStart="8dp"
    android:layout_marginTop="20dp"
    android:layout_marginEnd="8dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.666"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/imageView"
    app:srcCompat="@drawable/turismo" />


  <Button
    android:id="@+id/btnRedondo"
    android:layout_width="354dp"
    android:layout_height="57dp"
    android:layout_marginStart="8dp"
    android:layout_marginTop="72dp"

    android:layout_marginEnd="8dp"
    android:background="@drawable/borde_redondo"
    android:text="INICIAR SESIÃ“N"
    android:textColor="#FFFF"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.487"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/imageView4" />

  <TextView
    android:id="@+id/textView"
    android:layout_width="90dp"
    android:layout_height="35dp"
    android:layout_marginStart="44dp"
    android:layout_marginEnd="8dp"
    android:layout_marginBottom="88dp"
    android:text="Registro"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toStartOf="@+id/txtsaltar"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent" />

  <TextView
    android:id="@+id/txtsaltar"
    android:layout_width="99dp"
    android:layout_height="35dp"
    android:layout_marginBottom="88dp"
    android:text="Saltar"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent" />

</android.support.constraint.ConstraintLayout>